import string
import random

def random_string(length): 
   result = ''.join((random.choice(string.ascii_lowercase)) for x in range(10))
   #result = string.ascii_lowercase  
   return result


 def generate_dict():
   for i in range(100):
     print("'" + random_string(random.randint(1,5)) + "'" +  ": '" + random_string(random.randint(1,5)) + "'," )

 generate_dict()

