list = [8.30662386292, 20.493901532, 30.1827765456, 83.4805366538, 648.398025907]
print ("Original List:")
print(list)
list2 = list.reverse()
print ("Reversed List:")
print(list)
list2 = list.reverse()

print ("Concatenated List:")
list1 = ["my", "name", "is", "Bond"]
list2 = ["James", "Bond"]
list3 = list1 + list2
print(list3)

print ("Original List Squared:")
result = [i**2 for i in list]
print(result)
